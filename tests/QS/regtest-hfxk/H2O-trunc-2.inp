&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME EMSL_BASIS_SETS
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 200
      REL_CUTOFF 50
    &END MGRID
    &QS
      METHOD GAPW
    &END QS
    &SCF
      EPS_SCF 1.0E-6
      SCF_GUESS ATOMIC
      MAX_SCF 50
    &END SCF
    &XC
      &XC_FUNCTIONAL NONE
      &END XC_FUNCTIONAL
      &HF
        &HF_INFO ON
        &END
        PW_HFX T
        &SCREENING
          EPS_SCHWARZ 1.0E-7
          SCREEN_ON_INITIAL_P FALSE
        &END
        &INTERACTION_POTENTIAL
          POTENTIAL_TYPE TRUNCATED
          CUTOFF_RADIUS 2.0
          T_C_G_DATA t_c_g.dat
        &END
        &MEMORY
          MAX_MEMORY 10
        &END
        &PERIODIC
          NUMBER_OF_SHELLS 3
        &END
      &END
    &END XC
    &KPOINTS
       SCHEME  MONKHORST-PACK  1 1 1
       SYMMETRY ON
       EPS_GEO 1.e-8
       FULL_GRID ON
       VERBOSE F
       PARALLEL_GROUP_SIZE  0
    &END KPOINTS
  &END DFT
  &SUBSYS
    &CELL
      ABC 5.0 5.0 5.0
    &END CELL
    &COORD
    O   0.000000    0.000000   -0.065587
    H   0.000000   -0.757136    0.520545
    H   0.000000    0.757136    0.520545
    &END COORD
    &KIND H
      BASIS_SET 6-31Gxx
      POTENTIAL ALL
    &END KIND
    &KIND O
      BASIS_SET 6-31Gxx
      POTENTIAL ALL
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT H2O-trunc
  PRINT_LEVEL LOW
  RUN_TYPE ENERGY
&END GLOBAL
