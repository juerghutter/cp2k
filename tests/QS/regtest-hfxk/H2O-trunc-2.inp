&FORCE_EVAL
  METHOD Quickstep
  &DFT
    &MGRID
      CUTOFF 400
      REL_CUTOFF 60
    &END MGRID
    &QS
      METHOD GPW
    &END QS
    &SCF
      EPS_SCF 1.0E-6
      SCF_GUESS ATOMIC 
      MAX_SCF 2
    &END SCF
    &XC
      &XC_FUNCTIONAL NONE
      &END XC_FUNCTIONAL
      &HF
        &HF_INFO ON
        &END
        PW_HFX T
        &SCREENING
          EPS_SCHWARZ 1.0E-7
          SCREEN_ON_INITIAL_P FALSE
        &END
        &INTERACTION_POTENTIAL
          POTENTIAL_TYPE TRUNCATED
          CUTOFF_RADIUS 2.0
          T_C_G_DATA t_c_g.dat
        &END
        &MEMORY
          MAX_MEMORY 0
        &END
        &PERIODIC
          NUMBER_OF_SHELLS 3
        &END
      &END
    &END XC
    &KPOINTS
##       SCHEME GAMMA
       SCHEME MONKHORST-PACK   1 1 6
       SYMMETRY OFF
       EPS_GEO 1.e-8
       FULL_GRID ON
       VERBOSE F
       PARALLEL_GROUP_SIZE  0
##      WAVEFUNCTIONS REAL
    &END KPOINTS
  &END DFT
  &SUBSYS
    &CELL
      ABC 5.0 5.0 5.0
    &END CELL
    &COORD
    O   0.000000    0.000000   -0.065587
    H   0.000000   -0.757136    0.520545
    H   0.000000    0.757136    0.520545
    &END COORD
    &KIND H
      BASIS_SET DZV-GTH-PADE
      POTENTIAL GTH-PADE-q1
    &END KIND
    &KIND O
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-PADE-q6
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT H2O-trunc
  PRINT_LEVEL LOW
  RUN_TYPE ENERGY
&END GLOBAL
